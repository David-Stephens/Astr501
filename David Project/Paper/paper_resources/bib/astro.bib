journal = {\mnras},
keywords = {chemically peculiar,general,globular clusters,globular clusters: general,stars,stars: chemically peculiar},
month = aug,
number = {2},
pages = {1304--1309},
title = {{Formation of multiple populations in globular clusters: constraints on the dilution by pristine gas}},
url = {http://adsabs.harvard.edu/abs/2011MNRAS.415.1304D},
volume = {415},
